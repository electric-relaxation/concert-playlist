{
  "name": "concert-playlist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "venues:bottom:fixture": "tsx scripts/run-bottom-of-the-hill-fixture.ts",
    "venues:bottom:live": "tsx scripts/run-bottom-of-the-hill-live.ts",
    "venues:chapel:fixture": "tsx scripts/run-chapel-fixture.ts",
    "venues:chapel:live": "tsx scripts/run-chapel-live.ts",
    "venues:independent:fixture": "tsx scripts/run-independent-fixture.ts",
    "venues:independent:live": "tsx scripts/run-independent-live.ts",
    "data:update": "tsx scripts/updateVenueData.ts",
    "test:venue-update": "tsx scripts/test-venue-update.ts",
    "test:venue-update:clean": "node -e \"import('node:fs').then(fs=>fs.rmSync('scripts/tmp-venue-update',{recursive:true,force:true}))\"",
    "preview": "vite preview",
    "pages:dev": "npm run build && wrangler pages dev dist"
  },
  "dependencies": {
    "node-html-parser": "^7.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wrangler": "^4.54.0",
    "tsx": "^4.19.2"
  }
}
